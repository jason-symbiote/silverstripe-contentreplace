{
    "name": "jason-symbiote/silverstripe-contentreplace",
    "description": "This module replaces certain tags in WYSIWYG content with SilverStripe templates.",
    "type": "silverstripe-module",
    "keywords": [
        "silverstripe",
        "symbiote"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Jason Wang",
            "email": "jason@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/cms": "^4.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\silverstripe-contentreplace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symbiote\\silverstripe-contentreplace\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "expose": [
            "client/images"
        ],
        "installer-name": "silverstripe-contentreplace"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}