{
    "name": "jason-symbiote/silverstripe-contentreplace",
    "description": "This module replaces certain tags in WYSIWYG content with SilverStripe templates.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "symbiote"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Jason Wang",
            "email": "jason@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7"
    },
    "scripts": {
        "phpcbf": "phpcbf src/ tests/",
        "phpcs": "phpcs src/ tests/"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\silverstripe-contentreplace\\": "src/",
            "Symbiote\\silverstripe-contentreplace\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
